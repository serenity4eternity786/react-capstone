{
  "name": "react-capstone",
  "version": "1.0.0",
  "description": "",
  "main": "js/index.js",
  "scripts": {
    "start": "http-server build \"concurrently \"npm run server\" \"npm run client\"",
    "server": "babel-node server.js",
    "client": "babel-node start-client.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "lint": "eslint .",
    "dev": "run-p watch serve",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "build": "run-s build:*",
    "clean": "rimraf build",
    "serve": "npm start",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp build",
    "clean:html": "rimraf build/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "run-s clean:html mkdir:html _build:html",
    "_build:html": "cp index.html build",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar index.html -c \"npm run build:html\"",
    "clean:js": "rimraf build/js",
    "mkdir:js": "mkdirp build/js",
    "build:js": "run-s clean:js mkdir:js _build:js",
    "_build:js": "webpack",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch",
    "clean:css": "rimraf build/css",
    "mkdir:css": "run-s mkdir",
    "build:css": "run-s clean:css mkdir:css _build:css",
    "_build:css": "cpr css build/css",
    "watch:css": "run-s build:css _watch:css",
    "_watch:css": "chokidar css/**/* -c \"npm run build:css\"",
    "mkdir:assets": "run-s mkdir",
    "clean:assets": "rimraf build/assets",
    "build:assets": "run-s clean:assets mkdir:assets _build:assets",
    "_build:assets": "cpr assets build/assets",
    "watch:assets": "run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serenity4eternity786/react-capstone.git"
  },
  "author": "Alisha Byford",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/serenity4eternity786/react-capstone/issues"
  },
  "homepage": "https://github.com/serenity4eternity786/react-capstone#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "mocha": "^3.0.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.16.0",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "chokidar-cli": "^1.2.0",
    "concurrently": "3.1.0",
    "cpr": "^2.0.0",
    "npm-run-all": "^3.0.0",
    "rimraf": "^2.5.4",
    "enzyme": "^2.4.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "express": "4.13.3",
    "fs": "0.0.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sql.js": "0.3.2",
    "webpack": "^1.13.3"
  }
}

